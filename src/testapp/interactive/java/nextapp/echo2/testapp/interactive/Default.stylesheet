<?xml version="1.0" encoding="UTF-8"?>
<stylesheet>
 <style name="default" type="nextapp.echo2.app.Button">
  <properties>
   <property name="background" value="#4f4f5f"/>
   <property name="foreground" value="#efefff"/>
   <property name="border">
    <border size="1px" style="outset" color="#6f6f8f"/>
   </property>
   <property name="rolloverEnabled" value="true"/>
   <property name="rolloverBackground" value="#6f6f87"/>
   <property name="rolloverForeground" value="#ffffaf"/>
   <property name="rolloverBorder">
    <border size="1px" style="outset" color="#8f8faf"/>
   </property>
   <property name="pressedEnabled" value="true"/>
   <property name="pressedBackground" value="#2f2f3f"/>
   <property name="pressedForeground" value="#ffafaf"/>
   <property name="pressedBorder">
    <border size="1px" style="inset" color="#4f4f6f"/>
   </property>
   <property name="insets" value="20px 3px"/>
  </properties>
 </style>
 <style name="selectedButton" basename="default" type="nextapp.echo2.app.Button">
  <properties>
   <property name="background" value="#3f3f4f"/>
   <property name="foreground" value="#ffcfcf"/>
  </properties>
 </style>
 <style name="default" type="nextapp.echo2.app.text.TextComponent">
  <properties>
   <property name="background" value="#ffffdf"/>
   <property name="foreground" value="#000000"/>
   <property name="border">
    <border size="1px" style="inset" color="#ffffaf"/>
   </property>
  </properties>
 </style>
 <style name="titleLabel" type="nextapp.echo2.app.Label">
  <properties>
   <property name="font">
    <font typeface="Verdana" size="18px"/>
   </property>
   <property name="foreground" value="#ffffff"/>
   <property name="layoutData">
    <layoutdata type="nextapp.echo2.app.layout.SplitPaneLayoutData">
     <properties>
      <property name="alignment">
       <alignment horizontal="center"/>
      </property>
      <property name="background" value="#1f1f27"/>
      <property name="insets" value="8px"/>
      <property name="overflow" value="OVERFLOW_HIDDEN"/>
     </properties>
    </layoutdata>
   </property>
  </properties>
 </style>
 <style name="default" type="nextapp.echo2.app.WindowPane">
  <properties>
   <property name="titleInsets" value="10px 5px"/>
   <property name="border">
    <border size="2px" style="groove" color="#0f0f2f"/>
   </property>
   <property name="titleBackground" value="#2f2f4f"/>
   <property name="titleForeground" value="#ffffff"/>
  </properties>
 </style>
 <style name="applicationControlsRow" type="nextapp.echo2.app.Row">
  <properties>
   <property name="layoutData">
    <layoutdata type="nextapp.echo2.app.layout.SplitPaneLayoutData">
     <properties>
      <property name="background" value="#4f4f6f"/>
      <property name="insets" value="5px"/>
      <property name="minimumSize" value="200px"/>
      <property name="maximumSize" value="300px"/>
     </properties>
    </layoutdata>
   </property>
  </properties>
 </style>
 <style name="testControlsRow" type="nextapp.echo2.app.Row">
  <properties>
   <property name="layoutData">
    <layoutdata type="nextapp.echo2.app.layout.SplitPaneLayoutData">
     <properties>
      <property name="background" value="#afafef"/>
      <property name="insets" value="5px"/>
      <property name="minimumSize" value="200px"/>
      <property name="maximumSize" value="300px"/>
     </properties>
    </layoutdata>
   </property>
  </properties>
 </style>
</stylesheet>